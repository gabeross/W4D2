<form action="<%= cats_url %>" method="post">
  <label>
    name:
    <input type="text" name="cat[name]" value="">
  </label>
  <br>
  <label>
    color:
    <select name="cat[color]">
      <option value="select_a_color" selected>Select a color!</option>
      <option value="black">Black</option>
      <option value="brown">Bowwn</option>
      <option value="white">White</option>
      <option value="grey">Grey</option>
      <option value="Tabby">Tabby</option>
    </select>
  </label>
  <br>
  <label>
    birthday:
    <input type="date" name="cat[birth_date]" value="">
  </label>
  <br>
  <label>
    sex:
    <input type="radio" name="cat[sex]" value="male"> M <br>
    <input type="radio" name="cat[sex]" value="female"> F <br>
  </label>
  <br>
  <label>
    description:
    <textarea name="cat[description]" rows="10" cols="30">
        Tell us about your cat.
    </textarea>

  </label>
  <br>
  <<input type="submit" name ="" value="Create Your Cat!">
</form>